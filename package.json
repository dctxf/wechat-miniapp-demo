{
  "name": "wechat-miniapp-demo",
  "version": "1.0.0",
  "private": true,
  "description": "微信小程序模板",
  "templateInfo": {
    "name": "redux",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "MODE=development npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "prepare": "husky install",
    "version:check": "exec < /dev/tty && node ./scripts/check-version.js",
    "precommit": "lint-staged",
    "commit:verify": "commitlint -e $HUSKY_GIT_PARAMS",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix && npm run lint:prettier && npm run lint:style",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "prettier": "prettier -c --write \"src/**/*\""
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "dctxf",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@tarojs/components": "3.4.3",
    "@tarojs/plugin-framework-react": "3.4.3",
    "@tarojs/react": "3.4.3",
    "@tarojs/runtime": "3.4.3",
    "@tarojs/taro": "3.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@prefresh/babel-plugin": "^0.4.3",
    "@tarojs/mini-runner": "3.4.3",
    "@tarojs/plugin-platform-weapp": "^3.4.3",
    "@tarojs/webpack-runner": "3.4.3",
    "@types/react": "^17.0.44",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@umijs/fabric": "^2.10.2",
    "babel-preset-taro": "3.4.3",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-taro": "3.4.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "preact": "^10.7.1",
    "prettier": "^2.6.2",
    "react-refresh": "0.9.0",
    "stylelint": "9.3.0",
    "typescript": "^4.6.3",
    "webpack": "4.46.0"
  }
}
